<% provide(:title, "#{@user.company}") %>

<% content_for :open_graph do %>
	
	<meta property='og:title' content='<%= @user.company || 'PromoDigi' %> - <%= @landing_page.title %>' />
	<meta property='og:type' content='article' />
	<meta property='og:url' content='http://www.promodigi.net/' />
	<meta property='og:image' content='<%= @og_image %>' />

<% end %>

<%= render partial: "shared/landing_page", locals: { landing_page: @landing_page, user: @user, preview: false } %>